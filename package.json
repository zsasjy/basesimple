        "js-cookie": "^3.0.1",
        "less": "^4.1.3",
        "qs": "^6.10.5",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-redux": "^8.0.2",
        "react-router-dom": "^6.3.0",
        "redux": "^4.2.0",
        "redux-thunk": "^2.4.1",
        "uuid": "^8.3.2"
